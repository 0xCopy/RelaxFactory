<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:date="urn:import:com.google.gwt.user.datepicker.client">
    <ui:style>
        .label {
        }

    </ui:style>
    <g:HTMLPanel>
        <div>
            <label>Product Name:</label>
            <g:TextBox ui:field="product"/>
        </div>
        <div>
            <label>Product Description:</label>
            <g:TextArea ui:field="productDescription"/>
        </div>
        <div>
            <label>Limit:</label>
            <g:IntegerBox ui:field="limit"/>
        </div>
        <div>
            <label>Expires:</label>
            <date:DateBox ui:field="expire"></date:DateBox>
        </div>
        <div>
            <label>Vendor:</label>
            <g:ValueListBox ui:field="vendor"></g:ValueListBox>
        </div>
        <div>
            <label>Non-Profit Org:</label>
            <g:ValueListBox ui:field="npo"></g:ValueListBox>
        </div>
    </g:HTMLPanel>
</ui:UiBinder> 